{
  "name": "the-boss",
  "main": "index.js",
  "version": "0.1.4",
  "scripts": {
    "start": "npx expo start",
    "prebuild": "expo prebuild",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "test:ci": "jest --ci --coverage --maxWorkers=2 --passWithNoTests",
    "test": "jest --watchAll --coverage --passWithNoTests",
    "check:i18n": "tsx scripts/check-i18n.ts",
    "sync:i18n": "tsx scripts/sync-i18n.ts",
    "auto:i18n": "dotenv -e .env -- tsx scripts/auto-translate-i18n.ts",
    "typecheck": "npx tsc --noEmit",
    "lint": "eslint . --fix",
    "format": "prettier --write .",
    "check": "yarn typecheck &&yarn check:i18n && yarn lint && yarn format",
    "eas-build-pre-install": "corepack enable && yarn set version 4",
    "analyze:ios": "source-map-explorer 'dist/_expo/static/js/ios/*.js' 'dist/_expo/static/js/ios/*.js.map'",
    "analyze:android": "source-map-explorer 'dist/_expo/static/js/android/*.js' 'dist/_expo/static/js/android/*.js.map'"
  },
  "jest": {
    "preset": "jest-expo",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts",
      "!src/**/__tests__/**",
      "!**/node_modules/**",
      "!**/coverage/**"
    ]
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
